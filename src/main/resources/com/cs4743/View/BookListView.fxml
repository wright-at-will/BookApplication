<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<BorderPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <center>
      <VBox BorderPane.alignment="CENTER">
         <children>
            <ListView fx:id="bookList" prefHeight="234.0" prefWidth="398.0" />
         </children>
      </VBox>
   </center>
   <bottom>
      <HBox alignment="CENTER" prefHeight="55.0" prefWidth="398.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="deleteButton" contentDisplay="CENTER" mnemonicParsing="false" prefHeight="32.0" prefWidth="114.0" text="Delete" />
         </children>
      </HBox>
   </bottom>
</BorderPane>
